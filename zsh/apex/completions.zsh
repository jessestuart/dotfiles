#compdef apex
_apex() {
  COMPREPLY=()
  local cur="${COMP_WORDS[COMP_CWORD]}"
  local opts="$(apex autocomplete -- ${COMP_WORDS[@]:1})"
  echo $opts
  COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
  return 0
}
_apex "$@"

